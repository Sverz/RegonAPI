{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "declaration": true,
    "declarationMap": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "es6",
    "sourceMap": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
